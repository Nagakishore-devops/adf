{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "001devops",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "input//"
					},
					{
						"name": "Destination",
						"value": "input//"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureMySqlSource",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"quoteAllText": true
						}
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorage1",
							"type": "LinkedServiceReference"
						},
						"path": {
							"value": "@pipeline().Pipeline",
							"type": "Expression"
						},
						"enableCompression": true
					},
					"parallelCopies": 2,
					"enableSkipIncompatibleRow": false,
					"translator": {
						"value": "@pipeline().DataFactory",
						"type": "Expression"
					}
				},
				"inputs": [
					{
						"referenceName": "AzureMySqlTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference",
						"parameters": {
							"output": "v2"
						}
					}
				]
			}
		],
		"annotations": []
	}
}